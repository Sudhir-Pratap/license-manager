{
    "name": "insurance-core/validator",
    "description": "Enterprise-grade validation and protection package for Laravel applications",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "validation", "security", "protection", "insurance"],
    "homepage": "https://github.com/insurance-core/validator",
    "autoload": {
        "psr-4": {
            "InsuranceCore\\Validator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Insurance Core Team",
            "email": "support@insurance-core.com",
            "homepage": "https://insurance-core.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0", 
        "illuminate/cache": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/config": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/pipeline": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "symfony/http-foundation": "^6.0|^7.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "InsuranceCore\\Validator\\ValidatorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
